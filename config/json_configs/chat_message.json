{
  "type": "chat_message",
  "message_storage_config": {
    "type": "chat_message_storage",
    "cache_db_config": {
      "type": "redis",
      "host": "${REDIS_HOST}",
      "port": "${REDIS_PORT}",
      "db": "${REDIS_DB}",
      "password": "${REDIS_PASSWORD}"
    },
    "relational_db_config": {
      "type": "postgresql",
      "host": "${POSTGRES_HOST}",
      "port": "${POSTGRES_PORT}",
      "database": "${POSTGRES_DB}",
      "user": "${POSTGRES_USER}",
      "password": "${POSTGRES_PASSWORD}"
    }
  }
}
